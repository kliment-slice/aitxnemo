{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "excludeFiles": "{.next,.git,node_modules,__pycache__}/**",
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
